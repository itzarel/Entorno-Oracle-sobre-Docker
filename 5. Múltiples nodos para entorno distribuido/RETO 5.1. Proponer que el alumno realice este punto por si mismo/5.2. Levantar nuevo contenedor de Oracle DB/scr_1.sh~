#!/bin/bash

netstat -vap | grep 8082
netstat -vap | grep 1524

# If you use the same name container () you will get this message
#   docker: Error response from daemon: Conflict. The container name "/<CONTAINER_NAME>" is already in use by container "<ID>". You have to remove (or rename) that container to be able to reuse that name.
#   See 'docker run --help'.

docker run -d -p 8082:8080 -p 1524:1521 --name real-oracle-db-new-env store/oracle/database-enterprise:12.2.0.1
